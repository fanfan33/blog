<div class="pageDivide" aria-label="Page navigation">
        <ul class="pagination">
            
                <% if(page>0){ %>
                    <li><a href="/admin/user?page=<%= (page-1) %>" aria-label="Previous">
                        <span aria-hidden="true">&laquo;</span>
                    </a></li>
                <%}else{%>
                    <li class="disabled"><a href="#" aria-label="Previous">
                        <span aria-hidden="true">&laquo;</span>
                    </a></li>
                <%}%>
                
            <% for(var j = 0; j < totalpage; j++ ){ %>
                <% if(page==j){ %>
                    <li class="active"><span><%= j+1 %></span></li>
                <%}else{%>
                    <li><a href="/admin/user?page=<%= j %>"><%= j+1 %></a></li>
                <%}%>
                
            <% } %>

            <% if(page < totalpage-1){ %>
            <li>
                <a href="/admin/user?page=<%= (page+1) %>" aria-label="Next">
                    <span aria-hidden="true">&raquo;</span>
                </a>
            </li>
            <%}else{%>
                <li class="disabled">
                        <a href="#" aria-label="Next">
                            <span aria-hidden="true">&raquo;</span>
                        </a>
                    </li>
                <%}%>
        </ul>
    </div>